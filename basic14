#include <stdio.h>  
#include <string.h>  
signed main()  
{  
    char ch[69];  //將數字存進字元陣列
      
    scanf("%s",ch);  
    int l=strlen(ch),flag=1;    
      
    for(int i=0;i<l;i++)  
    {  
        if(ch[i]!=ch[l-i-1])  //判斷是否對稱
        {   
            flag=0;  
        }  
      
    }  
      
    if(flag)  
    {  
        printf("YES\n");  
    }  
    else   
        printf("NO\n");  
      
} 
