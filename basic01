#include <stdio.h>  
#include <stdlib.h>  
#include <string.h>  
  
void zero(char z[5][5])   
{  
    for (int i = 0; i < 5; i++)  
    { //*****  
        z[0][i] = '*';  
    }  
    for (int i = 1; i < 4; i++)  
    { //*   *  
        z[i][0] = '*';  
        for (int u = 1; u <= 3; u++)  
        {  
            z[i][u] = ' ';  
        }  
        z[i][4] = '*';  
    }  
  
    for (int i = 0; i < 5; i++)  
    { //*****  
        z[4][i] = '*';  
    }  
}  
  
void one(char on[5][5])  
{  
    for (int i = 0; i <= 4; i++)  
    {  
        for (int u = 0; u <= 4; u++)  
        {  
            on[i][u] = ' ';  
        }  
        on[i][4] = '*';  
    }  
}  
  
void two(char tw[5][5])  
{  
    //*****  
    for (int j = 0; j < 5; j++)  
    {  
        tw[0][j] = '*';  
    }  
  
    //....*  
    for (int j = 0; j < 4; j++)  
    {  
        tw[1][j] = ' ';  
    }  
    tw[1][4] = '*';  
  
    //*****  
    for (int j = 0; j < 5; j++)  
    {  
        tw[2][j] = '*';  
    }  
  
    //*....  
    tw[3][0] = '*';  
    for (int j = 1; j <= 4; j++)  
    {  
        tw[3][j] = ' ';  
    }  
  
    for (int j = 0; j < 5; j++)  
    { //*****  
        tw[4][j] = '*';  
    }  
}  
  
void three(char th[5][5])  
{  
    //*****  
    for (int j = 0; j < 5; j++)  
    {  
        th[0][j] = '*';  
    }  
  
    //....*  
    for (int j = 0; j < 4; j++)  
    {  
        th[1][j] = ' ';  
    }  
    th[1][4] = '*';  
  
    //*****  
    for (int j = 0; j < 5; j++)  
    {  
        th[2][j] = '*';  
    }  
  
    //....*  
    for (int j = 0; j < 4; j++)  
    {  
        th[3][j] = ' ';  
    }  
    th[3][4] = '*';  
  
    //*****  
    for (int j = 0; j < 5; j++)  
    {  
        th[4][j] = '*';  
    }  
}  
  
void four(char fu[5][5])  
{  
    //*...*  
    fu[0][0] = '*';  
    for (int j = 1; j < 4; j++)  
    {  
        fu[0][j] = ' ';  
    }  
    fu[0][4] = '*';  
  
    //*...*  
    fu[1][0] = '*';  
    for (int j = 1; j < 4; j++)  
    {  
        fu[1][j] = ' ';  
    }  
    fu[1][4] = '*';  
  
    //*****  
    for (int j = 0; j < 5; j++)  
    {  
        fu[2][j] = '*';  
    }  
  
    //....*  
    for (int j = 0; j < 4; j++)  
    {  
        fu[3][j] = ' ';  
    }  
    fu[3][4] = '*';  
  
    //....*  
    for (int j = 0; j < 4; j++)  
    {  
        fu[4][j] = ' ';  
    }  
    fu[4][4] = '*';  
}  
  
void five(char fi[5][5])  
{  
    //*****  
    for (int j = 0; j < 5; j++)  
    {  
        fi[0][j] = '*';  
    }  
  
    //*....  
    fi[1][0] = '*';  
    for (int j = 1; j <= 4; j++)  
    {  
        fi[1][j] = ' ';  
    }  
  
    //*****  
    for (int j = 0; j < 5; j++)  
    {  
        fi[2][j] = '*';  
    }  
  
    //....*  
    for (int j = 0; j < 4; j++)  
    {  
        fi[3][j] = ' ';  
    }  
    fi[3][4] = '*';  
  
    //*****  
    for (int j = 0; j < 5; j++)  
    {  
        fi[4][j] = '*';  
    }  
}  
  
void six(char si[5][5])  
{  
    //*****  
    for (int j = 0; j < 5; j++)  
    {  
        si[0][j] = '*';  
    }  
  
    //*....  
    si[1][0] = '*';  
    for (int j = 1; j <= 4; j++)  
    {  
        si[1][j] = ' ';  
    }  
  
    //*****  
    for (int j = 0; j < 5; j++)  
    {  
        si[2][j] = '*';  
    }  
  
    //*...*  
    si[3][0] = '*';  
    for (int j = 1; j < 4; j++)  
    {  
        si[3][j] = ' ';  
    }  
    si[3][4] = '*';  
  
    //*****  
    for (int j = 0; j < 5; j++)  
    {  
        si[4][j] = '*';  
    }  
}  
  
void seven(char se[5][5])  
{  
    //*****  
    for (int j = 0; j < 5; j++)  
    {  
        se[0][j] = '*';  
    }  
  
    for (int i = 1; i <= 4; i++)  
    {  
        for (int u = 0; u <= 3; u++)  
        {  
            se[i][u] = ' ';  
        }  
        se[i][4] = '*';  
    }  
}  
  
void eight(char ig[5][5])  
{  
    //*****  
    for (int j = 0; j < 5; j++)  
    {  
        ig[0][j] = '*';  
    }  
  
    //*...*  
    ig[1][0] = '*';  
    for (int j = 1; j < 4; j++)  
    {  
        ig[1][j] = ' ';  
    }  
    ig[1][4] = '*';  
  
    //*****  
    for (int j = 0; j < 5; j++)  
    {  
        ig[2][j] = '*';  
    }  
  
    //*...*  
    ig[3][0] = '*';  
    for (int j = 1; j < 4; j++)  
    {  
        ig[3][j] = ' ';  
    }  
    ig[3][4] = '*';  
  
    //*****  
    for (int j = 0; j < 5; j++)  
    {  
        ig[4][j] = '*';  
    }  
}  
  
void nine(char ni[5][5])  
{  
    //*****  
    for (int j = 0; j < 5; j++)  
    {  
        ni[0][j] = '*';  
    }  
  
    //*...*  
    ni[1][0] = '*';  
    for (int j = 1; j < 4; j++)  
    {  
        ni[1][j] = ' ';  
    }  
    ni[1][4] = '*';  
  
    //*****  
    for (int j = 0; j < 5; j++)  
    {  
        ni[2][j] = '*';  
    }  
  
    //....*  
    for (int j = 0; j < 4; j++)  
    {  
        ni[3][j] = ' ';  
    }  
    ni[3][4] = '*';  
  
    //....*  
    for (int j = 0; j < 4; j++)  
    {  
        ni[4][j] = ' ';  
    }  
    ni[4][4] = '*';  
}  
  
char f[5][5];  
char u[5][5];  
char c[5][5];  
char k[5][5];  
char yu[5][20];  
  
int main()  
{  
    int n, m, s, l;  
    while(scanf("%1d%1d%1d%1d", &n, &m, &s, &l) != EOF)  
    {  
        if (n == 0)  
            zero(f);  
        if (n == 1)  
            one(f);  
        if (n == 2)  
            two(f);  
        if (n == 3)  
            three(f);  
        if (n == 4)  
            four(f);  
        if (n == 5)  
            five(f);  
        if (n == 6)  
            six(f);  
        if (n == 7)  
            seven(f);  
        if (n == 8)  
            eight(f);  
        if (n == 9)  
            nine(f);  
  
        if (m == 0)  
            zero(u);  
        if (m == 1)  
            one(u);  
        if (m == 2)  
            two(u);  
        if (m == 3)  
            three(u);  
        if (m == 4)  
            four(u);  
        if (m == 5)  
            five(u);  
        if (m == 6)  
            six(u);  
        if (m == 7)  
            seven(u);  
        if (m == 8)  
            eight(u);  
        if (m == 9)  
            nine(u);  
  
        if (s == 0)  
            zero(c);  
        if (s == 1)  
            one(c);  
        if (s == 2)  
            two(c);  
        if (s == 3)  
            three(c);  
        if (s == 4)  
            four(c);  
        if (s == 5)  
            five(c);  
        if (s == 6)  
            six(c);  
        if (s == 7)  
            seven(c);  
        if (s == 8)  
            eight(c);  
        if (s == 9)  
            nine(c);  
  
        if (l == 0)  
            zero(k);  
        if (l == 1)  
            one(k);  
        if (l == 2)  
            two(k);  
        if (l == 3)  
            three(k);  
        if (l == 4)  
            four(k);  
        if (l == 5)  
            five(k);  
        if (l == 6)  
            six(k);  
        if (l == 7)  
            seven(k);  
        if (l == 8)  
            eight(k);  
        if (l == 9)  
            nine(k);  
  
        for (int i = 0; i < 5; i++)  
        {  
            for (int j = 0; j < 5; j++)  
            {  
                printf("%c", f[i][j]);  
            }  
            printf(" ");  
            for (int j = 0; j < 5; j++)  
            {  
                printf("%c", u[i][j]);  
            }  
            printf(" ");  
            for (int j = 0; j < 5; j++)  
            {  
                printf("%c", c[i][j]);  
            }  
            printf(" ");  
            for (int j = 0; j < 5; j++)  
            {  
                printf("%c", k[i][j]);  
            }  
            //printf(" ");  
            printf("\n");  
        }  
    }  
}  
